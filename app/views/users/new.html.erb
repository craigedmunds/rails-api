<h1>New User</h1>
<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>
 
  <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </p>
 
  <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<p><%= link_to 'Back to list', users_path %></p>